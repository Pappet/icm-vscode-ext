  "fields": [
    {
      "name": "Range",
      "type": "Enum:RangeOptions | <Zeitbereich>",
      "doc": "Gibt den gewünschten Zeitbereich an. Erlaubt sind feste Werte (`Context`, `All`, `Past`, `Future`) oder eine dynamische Definition wie `ADM...DIS` oder `CTX-24h...CTX`."
    },
    {
      "name": "Records",
      "type": "Enum:RecordsOptions",
      "doc": "Filtert Datensätze, die in den Zeitbereich fallen. Mögliche Werte: All, First, Last, First+Last, Distinct, Open, Assigned PreOP."
    },
    {
      "name": "Sort",
      "type": "string",
      "doc": "Gibt die Sortierreihenfolge an. Syntax: '[-]Feldbezeichner1,[-]Feldbezeichner2'."
